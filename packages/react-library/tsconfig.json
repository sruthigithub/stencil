{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
      "outDir": "./dist",
      "lib": ["dom", "es2015"],
      "module": "esnext",
      "moduleResolution": "bundler",
      "target": "es2015",
      "skipLibCheck": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "declarationDir": "./dist/types",
      "paths": {
        "stencil-library/*": ["../stencil-library/dist"]
      }
    },
    "include": ["lib"],
    "exclude": ["node_modules"]
  }